class Animal {
  string name;

  void sayName() {
    printString(name);
  }
}

class Dog extends Animal {
  string breed;

  void sayBreed() {
    printString(breed);
  }
}

int main() {
  Animal animal = new Animal;
  animal.name = "anim";
  animal.sayName();
  
  Dog dog = new Dog;
  dog.name = "doggo";
  dog.breed = "owczarek";
  dog.sayBreed();
  dog.sayName();
  return 0;
}